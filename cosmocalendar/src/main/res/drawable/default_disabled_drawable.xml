<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <rotate
        android:fromDegrees="135"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toDegrees="135">
      <shape
          android:shape="line"
          android:top="1dip">
        <stroke
            android:color="@color/default_disabled_day_text_color"
            android:width="1dp"/>
      </shape>
    </rotate>
  </item>
</layer-list>